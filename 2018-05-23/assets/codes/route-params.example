const User = ({ match }) => <div>User: {match.params.user}</div>

const App = (
  <BrowserRouter>
    <div>
      <ul>
        <li><Link to="/kim">Kim (dynamic)</Link></li>
        <li><Link to="/chris">Chris (dynamic)</Link></li>
      </ul>

      <Route path="/users/:user" component={User} />
    </div>
  </BrowserRouter>
);
